<div class="container w-75">
  <h1>Add your properties</h1>

  <%= simple_form_for @listing do |f| %>
    <%= f.input :name %>
    <%= f.input :location %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :number_guests %>
    <%= f.submit "Add property", class: "btn btn-primary" %>
  <% end %>
</div>
